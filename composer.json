{
  "name": "bricev/compolab",
  "description": "Composer Repository for GitLab",
  "minimum-stability": "dev",
  "license": "proprietary",
  "require": {
    "php": "^7.1",
    "m4tthumphrey/php-gitlab-api": "^9.0",
    "php-http/guzzle6-adapter": "^1.0",
    "symfony/config": "^4.0",
    "symfony/console": "^4.0",
    "symfony/dependency-injection": "^4.0",
    "symfony/event-dispatcher": "^4.0",
    "symfony/http-kernel": "^4.0",
    "symfony/routing": "^4.0",
    "symfony/yaml": "^4.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^6"
  },
  "autoload": {
    "psr-4": {
      "CompoLab\\": "src",
      "CompoLab\\Tests\\": "tests/src"
    }
  },
  "scripts": {
    "post-create-project-cmd": [
      "@php bin/install"
    ]
  }
}
